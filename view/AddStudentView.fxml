<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="480.0" prefWidth="360.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AddStudentViewController">
   <children>
      <HBox alignment="CENTER" prefHeight="75.0" prefWidth="332.0">
         <children>
            <Label text="Add Student">
               <font>
                  <Font size="30.0" />
               </font>
               <HBox.margin>
                  <Insets top="8.0" />
               </HBox.margin>
            </Label>
         </children>
      </HBox>
      <HBox prefHeight="217.0" prefWidth="265.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <VBox alignment="TOP_CENTER" prefHeight="217.0" prefWidth="175.0">
                     <children>
                        <Label alignment="CENTER" prefHeight="33.0" prefWidth="135.0" text="Name">
                           <font>
                              <Font size="23.0" />
                           </font>
                           <VBox.margin>
                              <Insets top="10.0" />
                           </VBox.margin>
                        </Label>
                        <Label alignment="CENTER" layoutX="10.0" layoutY="10.0" prefHeight="33.0" prefWidth="135.0" text="ID">
                           <font>
                              <Font size="23.0" />
                           </font>
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                        </Label>
                        <Label alignment="CENTER" prefHeight="33.0" prefWidth="135.0" text="Semester">
                           <font>
                              <Font size="23.0" />
                           </font>
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                        </Label>
                        <Label alignment="CENTER" prefHeight="33.0" prefWidth="135.0" text="Class">
                           <font>
                              <Font size="23.0" />
                           </font>
                           <VBox.margin>
                              <Insets top="20.0" />
                           </VBox.margin>
                        </Label>
                     </children>
                     <padding>
                        <Insets top="5.0" />
                     </padding>
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                  </VBox>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </HBox>
            <HBox prefHeight="217.0" prefWidth="225.0">
               <children>
                  <VBox prefHeight="217.0" prefWidth="147.0">
                     <children>
                        <TextField fx:id="name" prefHeight="31.0" prefWidth="147.0">
                           <VBox.margin>
                              <Insets top="22.0" />
                           </VBox.margin>
                        </TextField>
                        <TextField fx:id="id" prefHeight="34.0" prefWidth="147.0">
                           <VBox.margin>
                              <Insets top="19.0" />
                           </VBox.margin>
                        </TextField>
                        <TextField fx:id="semester" prefHeight="34.0" prefWidth="147.0">
                           <VBox.margin>
                              <Insets top="19.0" />
                           </VBox.margin>
                        </TextField>
                        <TextField fx:id="myClass" prefHeight="33.0" prefWidth="147.0">
                           <VBox.margin>
                              <Insets top="19.0" />
                           </VBox.margin>
                        </TextField>
                     </children>
                  </VBox>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </HBox>
         </children>
      </HBox>
      <HBox prefHeight="87.0" prefWidth="360.0">
         <children>
            <HBox prefHeight="96.0" prefWidth="271.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#submitButton" prefHeight="48.0" prefWidth="97.0" text="ADD">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="30.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets top="25.0" />
               </HBox.margin>
            </HBox>
            <HBox prefHeight="63.0" prefWidth="349.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#backButton" prefHeight="56.0" prefWidth="95.0" text="Cancel">
                     <font>
                        <Font size="15.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="10.0" top="25.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <HBox.margin>
                  <Insets top="20.0" />
               </HBox.margin>
            </HBox>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <Label fx:id="error" prefHeight="33.0" prefWidth="352.0" text="IF SOMETHING GOES WRONG YOU ">
         <VBox.margin>
            <Insets left="20.0" top="20.0" />
         </VBox.margin>
      </Label>
   </children>
</VBox>
